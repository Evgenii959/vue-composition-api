{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "#app/*": ["./node_modules/nuxt/dist/app/*"],
      "#components/*": ["./components/*"],
      "#layouts/*": ["./layouts/*"],
      "#pages/*": ["./pages/*"],
      "#plugins/*": ["./plugins/*"],
      "#store/*": ["./store/*"]
    }
  }
}